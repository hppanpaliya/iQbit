{
  "name": "iqbit",
  "version": "1.0.1",
  "private": false,
  "homepage": "./",
  "dependencies": {
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@jridgewell/gen-mapping": "0.3.5",
    "@tanstack/react-query": "^5.0.0",
    "@types/fuzzyset.js": "^0.0.2",
    "@types/jest": "^27.0.1",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^22.12.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "ajv": "^8.16.0",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "eslint": "^8.11.0",
    "express": "^4.18.1",
    "filesize": "^9.0.11",
    "framer-motion": "^6",
    "fuzzyset.js": "^1.0.7",
    "iso-639-1": "^2.1.15",
    "js-cookie": "^3.0.1",
    "morgan": "^1.10.0",
    "moviedb-promise": "^3.1.18",
    "prettier": "^2.6.0",
    "react": "18.2.0",
    "react-activity-rings": "^1.1.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "18.2.0",
    "react-ga": "^3.3.1",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.2",
    "react-virtualized": "^9.22.6",
    "typescript": "^5.0.0",
    "usehooks-ts": "^2.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "vite dev --host --port 3000",
    "build": "vite build && cp ./release/public/index.html ./release/public/login.html",
    "lint": "eslint src",
    "preview": "vite preview",
    "winBuild": "vite build && powershell Remove-Item -Recurse -Force -Confirm:$false ./release/public/* && powershell mv build/* ./release/public && powershell cp release/public/index.html release/public/login.html",
    "server-setup": "cd server && npm install ",
    "server-start": "cd server && npm run serve",
    "server-docker-start": "cd server && npm run serve:docker",
    "server-stop": "cd server && npm run stop"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-empty-object-type": "off",
      "@typescript-eslint/no-unused-expressions": "off",
      "prefer-const": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@types/react-virtualized": "^9.21.21",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "vite": "^7.0.6"
  }
}
